{
  "name": "valhala-of-valkyrie",
  "version": "0.0.0",
  "description": "Ailrun's GitHub Blog",
  "scripts": {
    "build": "npx -n \"-r ts-node/register\" gatsby build",
    "check": "npm-run-all --serial checker:*",
    "checker:compile": "tsc --noEmit -p .",
    "checker:lint": "eslint . --ext ts --ext tsx",
    "release": "gh-pages -d public -t -b master -m \"Update from Gatsby\" -o Ailrun",
    "serve": "npx -n \"-r ts-node/register\" gatsby serve",
    "start": "npx -n \"-r ts-node/register\" gatsby develop",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "codemirror": "^5.54.0",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gatsby": "^2.21.33",
    "gatsby-plugin-catch-links": "^2.3.1",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-custom-eslint": "file:./plugins/gatsby-plugin-custom-eslint",
    "gatsby-plugin-custom-ts-checker": "file:./plugins/gatsby-plugin-custom-ts-checker",
    "gatsby-plugin-emotion": "^4.3.1",
    "gatsby-plugin-google-gtag": "^2.1.1",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-offline": "^3.2.2",
    "gatsby-plugin-preload-fonts": "^1.2.2",
    "gatsby-plugin-react-helmet-async": "^1.0.16",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-autolink-headers": "^2.3.2",
    "gatsby-remark-katex": "^3.3.1",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-json": "^2.4.2",
    "gatsby-transformer-remark": "^2.8.8",
    "gh-pages": "^2.2.0",
    "katex": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "parsimmon": "^1.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.95",
    "@types/parsimmon": "^1.10.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.13"
  },
  "private": true
}
