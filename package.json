{
  "name": "valhala-of-valkyrie",
  "version": "0.0.0",
  "description": "Ailrun's GitHub Blog",
  "scripts": {
    "build": "npx -n \"-r ts-node/register\" gatsby build",
    "check": "npm-run-all --serial checker:*",
    "checker:compile": "tsc --noEmit -p .",
    "checker:lint": "eslint . --ext ts --ext tsx",
    "release": "gh-pages -d public -t -b master -m \"Update from Gatsby\" -o Ailrun",
    "serve": "npx -n \"-r ts-node/register\" gatsby serve",
    "start": "npx -n \"-r ts-node/register\" gatsby develop",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/codemirror": "0.0.88",
    "@types/parsimmon": "^1.10.1",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "codemirror": "^5.52.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gatsby": "^2.19.49",
    "gatsby-plugin-catch-links": "^2.1.28",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-custom-eslint": "file:./plugins/gatsby-plugin-custom-eslint",
    "gatsby-plugin-emotion": "^4.1.25",
    "gatsby-plugin-google-gtag": "^1.1.24",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-offline": "^3.0.42",
    "gatsby-plugin-preload-fonts": "^1.0.36",
    "gatsby-plugin-react-helmet-async": "^1.0.16",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.2.30",
    "gatsby-plugin-typescript": "^2.2.5",
    "gatsby-remark-autolink-headers": "^2.1.26",
    "gatsby-remark-katex": "^3.1.26",
    "gatsby-source-filesystem": "^2.1.57",
    "gatsby-transformer-json": "^2.2.28",
    "gatsby-transformer-remark": "^2.6.59",
    "gh-pages": "^2.2.0",
    "katex": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "parsimmon": "^1.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.4",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "private": true
}
