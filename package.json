{
  "name": "valhala-of-valkyrie",
  "version": "0.0.0",
  "description": "Ailrun's GitHub Blog",
  "scripts": {
    "build": "npx -n \"-r ts-node/register\" gatsby build",
    "check": "npm-run-all --serial checker:*",
    "checker:compile": "tsc --noEmit -p .",
    "checker:lint": "eslint . --ext ts --ext tsx",
    "release": "gh-pages -d public -t -b master -m \"Update from Gatsby\" -o Ailrun",
    "serve": "npx -n \"-r ts-node/register\" gatsby serve",
    "start": "npx -n \"-r ts-node/register\" gatsby develop",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/codemirror": "0.0.87",
    "@types/parsimmon": "^1.10.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "codemirror": "^5.52.0",
    "disqus-react": "^1.0.7",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gatsby": "^2.19.28",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-custom-eslint": "file:./plugins/gatsby-plugin-custom-eslint",
    "gatsby-plugin-emotion": "^4.1.23",
    "gatsby-plugin-google-gtag": "^1.1.22",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-preload-fonts": "^1.0.29",
    "gatsby-plugin-react-helmet-async": "^1.0.14",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-typescript": "^2.2.0",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-remark": "^2.6.53",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "parsimmon": "^1.13.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.4",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "private": true
}
